{
  "name": "ov25-ui",
  "version": "0.1.2",
  "description": "A UI configurator for OV25 product visualization",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/bundle/ov25-ui.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "build:lib": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:bundle": "mkdir -p dist/bundle && rollup -c rollup.config.mjs",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build:lib && npm run build:bundle",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-scroll-area": "^1.2.5",
    "@radix-ui/react-select": "^2.2.2",
    "@react-spring/web": "^9.7.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@tailwindcss/vite": "^4.1.4",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^18.0.0",
    "@use-gesture/react": "^10.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.503.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-qr-code": "^2.0.15",
    "rimraf": "^3.0.2",
    "rollup": "^4.13.0",
    "tailwind": "^4.0.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^4.9.0",
    "vite": "^6.2.0"
  },
  "keywords": [
    "ov25",
    "ui",
    "configurator",
    "react"
  ],
  "author": "",
  "license": "MIT"
}
